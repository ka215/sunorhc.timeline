{
  "name": "sunorhc.timeline",
  "private": true,
  "version": "0.9.1",
  "type": "module",
  "description": "Timeline generator libraly",
  "main": "main.js",
  "scripts": {
    "dev": "vite --host --port 3333",
    "build": "tsc && vite build --mode Timeline",
    "build:tester": "tsc && vite build --mode Tester",
    "preview": "vite preview",
    "lint:code": "biome lint ./",
    "lint:style": "biome format ./",
    "fix:style": "biome format --write ./",
    "fix:code": "biome lint --apply ./",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ka215/sunorhc-timeline"
  },
  "keywords": [
    "timeline"
  ],
  "author": "Ka2",
  "license": "MIT",
  "bugs": {
    "url": "https://ka2.org/"
  },
  "homepage": "",
  "packageManager": "pnpm@9.0.4",
  "browserslist": [
    "last 2 versions",
    "android >= 4",
    "iOS >= 10",
    "not dead"
  ],
  "devDependencies": {
    "@biomejs/biome": "1.7.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@types/jquery": "^3.5.30",
    "@types/node": "^20.12.13",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "css-declaration-sorter": "^7.2.0",
    "happy-dom": "^14.7.1",
    "jquery": "^3.7.1",
    "jsdom": "^24.0.0",
    "postcss-sort-media-queries": "^5.2.0",
    "sass": "^1.75.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "lz-string": "^1.5.0",
    "pino": "^9.1.0",
    "pino-pretty": "^11.1.0",
    "ts-deepmerge": "^7.0.0",
    "winston": "^3.13.0"
  }
}
